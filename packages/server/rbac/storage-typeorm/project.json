{
  "name": "@owl-app/rbac-storage-typeorm",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/server/rbac/storage-typeorm/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/packages/server/rbac/storage-typeorm"],
      "options": {
        "commands": [
          "npx rimraf dist/packages/server/rbac/storage-typeorm",
          "npx tsc -p packages/server/rbac/storage-typeorm/tsconfig.build.json",
          "npx copyfiles -f \"packages/server/rbac/storage-typeorm/package.json\" \"packages/server/rbac/storage-typeorm/README.md\" \"LICENSE\" dist/packages/server/rbac/storage-typeorm"
        ],
        "cwd": ".",
        "parallel": false
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx copyfiles -f \"dist/packages/server/rbac/storage-typeorm/src/*\" dist/packages/server/rbac/storage-typeorm/dist",
            "forwardAllArgs": false
          },
          {
            "command": "npx rimraf dist/packages/server/rbac/storage-typeorm/src",
            "forwardAllArgs": false
          },
          "node tools/scripts/publish.mjs @owl-app/registry-nestjs {args.ver} {args.tag}"
        ],
        "cwd": ".",
        "parallel": false,
        "packageDir": "{workspaceRoot}/dist/packages/server/rbac/storage-typeorm/"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": []
}


