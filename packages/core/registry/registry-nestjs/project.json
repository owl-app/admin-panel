{
  "name": "@owl-app/registry-nestjs",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/core/registry/registry-nestjs/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{workspaceRoot}/dist/packages/core/registry/registry-nestjs/"],
      "options": {
        "commands": [
          "npx rimraf dist/packages/core/registry/registry-nestjs",
          "npx tsc -p packages/core/registry/registry-nestjs/tsconfig.build.json",
          "npx copyfiles -f \"packages/core/registry/registry-nestjs/package.json\" \"packages/core/registry/registry-nestjs/README.md\" \"LICENSE\" dist/packages/core/registry/registry-nestjs"
        ],
        "cwd": ".",
        "parallel": false
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx copyfiles -f \"dist/packages/core/registry/registry-nestjs/src/*\" dist/packages/core/registry/registry-nestjs/dist",
            "forwardAllArgs": false
          },
          {
            "command": "npx rimraf dist/packages/core/registry/registry-nestjs/src",
            "forwardAllArgs": false
          },
          "node tools/scripts/publish.mjs @owl-app/registry-nestjs {args.ver} {args.tag}"
        ],
        "cwd": ".",
        "parallel": false,
        "packageDir": "{workspaceRoot}/dist/packages/core/registry/registry-nestjs/"
      },
      "dependsOn": ["build"]
    }
  },
  "tags": []
}
